@model IEnumerable<DealersTrack.Models.Lendor>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Dealer.dealerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lendorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lendorAddress)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Dealer.dealerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lendorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lendorAddress)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.lendorId }) |
            @Html.ActionLink("Details", "Details", new { id=item.lendorId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.lendorId })
        </td>
    </tr>
}

</table>
